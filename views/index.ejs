<!DOCTYPE html>


<html>
  <head>
    <title><% title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <ul>
      <% for(var i=0; i < messages.length; i++) { %>
        <tr>
          <td><%= messages[i].user %></td>
          <td><%= messages[i].text %></td>
          <td><%= messages[i].added %></td>
        </tr>
        <br>
     <% } %>
    </ul>
    <button onclick="window.location.href = window.location.href+'new'">Click Here</button>
  </body>
</html>
